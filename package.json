    "build:ssr": "ng build --prod --output-hashing=all && ng run spotbie:server:production",
    "prerender": "ng run spotbie:prerender",
    "build:android": "ng build --prod --base-href . --output-path /home/spotbie/Desktop/spotbie/spotbie_android/www/",
