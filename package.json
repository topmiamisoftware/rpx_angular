{
  "name": "spotbie",
  "displayName": "SpotBie Web App",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --ssl --proxy-config proxy.conf.json",
    "build": "ng build",
    "post-build": "node ./build/post-build.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run spotbie:serve-ssr",
    "serve:ssr": "node dist/spotbie/server/main.js",
    "build:ssr": "ng build --prod --output-hashing=all && ng run spotbie:server:production",
    "prerender": "ng run spotbie:prerender",
    "build:android": "ng build --prod --base-href . --output-path /home/spotbie/Desktop/spotbie/spotbie_android/www/",
    "build:ios": "ng build --prod --base-href . --output-path ~/Desktop/spotbie/spotbie_ios/www/",
    "deploy:server": "node deploy"
  },
